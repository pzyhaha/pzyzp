//1.创建mymax函数，在函数里比较两个成绩。
//2.在main函数中，输出最高成绩。
#include <iostream>
using namespace std;
double mymax(double score1, double score2);

int main()
{
	double score1 = 0;
	double score2 = 0;
	cout << "请输入两门成绩：" << endl;
	cin >> score1;
	cin >> score2;
	double max = (score1, score2);
	cout << "最高成绩为：" << max<< endl;
}

double mymax(double score1, double score2)
{
	if (score1 > score2)
	{
		return score1;
	}
	else
	{
		return score2;
	}
}
